{
  "version": 2,
  "builds": [
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/index", "dest": "/public/index.html" },
    { "src": "/index.html", "dest": "/public/index.html" },
    { "src": "/api/upload", "dest": "/api/upload.js" }
  ]
}
